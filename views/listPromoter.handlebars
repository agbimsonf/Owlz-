<h1>list</h1>

<div id="list">
    {{#each promoters}}
    <ul>
        <li>{{this.first_name}}</li>
        <li>{{this.city}}</li>
    </ul>
    <!-- Button trigger modal -->
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal">
  Contact
</button>

<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Contact</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form class="create-form">
            <div class="form-group">
                <label>First name</label>
                <input type="text" id="firstName" value="{{promoter.first_name}}">
            </div>
                <div class="form-group">
                <label>Last name</label>
                <input type="text" id="lastName" value="{{promoter.last_name}}">
            </div>
              <div class="form-group">
                <label>Handle</label>
                <input type="text" id="handle" value="{{promoter.handle}}">
            </div>
              <div class="form-group">
                <label>About</label>
                <input type="text" id="about" value="{{promoter.descriptions}}">
            </div>
              <div class="form-group">
                <label>City</label>
                <input type="text" id="city" value="{{promoter.city}}">
            </div>
                <div class="form-group">
                <label>Language</label>
                <input type="text" id="language" value="{{promoter.languages}}">
            </div>
                <div class="form-group">
                <label>Phone Number</label>
                <input type="text" id="phone" value="{{promoter.phone_number}}">
            </div>
                <div class="form-group">
                <label>Email</label>
                <input type="email" id="email" value="{{promoter.email}}">
            </div>
              <div class="form-group">
                <label>Instagram</label>
                <input type="text" id="insta" value="{{promoter.instagram}}">
            </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button class="updatePromoter" type="button" data-id="{{promoter.id}}">Save changes</button>
      </div>
    </div>
  </div>
</div>
    {{/each}}
</div>
