{{#each promoters}}
<div class="container">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                                <div class="col-3 text-left">
                                    <img src="/assets/images/profile.png" alt="profile photo" class="profilePhoto">
                                    <br>
                                    <br>
                                    <h5>email: {{this.email}}</h5>
                                    <h5>languages: {{this.languages}}</h5>
                                    <a href="https://www.instagram.com/{{this.instagram}}"><i class="fab fa-instagram fa-2x"></i></a>
                                    {{!-- <i class="fab fa-twitter-square fa-2x"></i> --}}
                                </div>                                    
                            <div class="col-9">
                                <h3 class="card-title">handle: {{this.handle}}</h3>
                                <h5>city: {{this.city}} </h5>
                                <h5>name: {{this.first_name}} {{this.last_name}}</h5>
                                <h5>about: </h5> 
                                <p>{{this.descriptions}}</p>
                                <button type="button" class="btn btn-dark" data-toggle="modal" data-target="#exampleModal{{this.id}}">
                                  Contact Me
                                </button>
                            </div>
                          
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <hr class="style-eight">
  
        <br>
        <br>

<!-- Modal -->
<div class="modal fade" id="exampleModal{{this.id}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Contact</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form class="create-form">
            <div class="form-group">
                <label>Start date</label>
                <input type="date" id="start_date{{this.id}}">
            </div>
                <div class="form-group">
                <label>End date</label>
                <input type="date" id="end_date{{this.id}}">
            </div>
              <div class="form-group">
                <label>Number of guests</label>
                <input type="number" id="guests{{this.id}}">
            </div>
              <div class="form-group">
                <label>Occasion</label>
                <input type="text" id="occasion{{this.id}}">
            </div>
              <div class="form-group">
                <label>Message</label>
                <input type="text" id="message{{this.id}}">
            </div>
                <div class="form-group">
                <label>Email</label>
                <input type="email" id="email_client{{this.id}}">
            </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button class="contactPromoter btn btn-dark" type="button" data-id="{{this.id}}" data-dismiss="modal">Send</button>
      </div>
    </div>
  </div>
</div>
{{/each}}
</div>

